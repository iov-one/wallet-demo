{
  "name": "wallet-demo",
  "version": "0.9.0",
  "description": "Demo web-wallet built on iov-core",
  "main": "build/index.js",
  "author": "Ethan Frey <ethanfrey@users.noreply.github.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "tslint -t verbose --project .",
    "format": "prettier --write --loglevel warn \"./src/**/*.{ts,tsx}\"",
    "prebuild": "yarn format && yarn lint",
    "build": "shx rm -rf ./build && tsc --module commonjs",
    "test": "NODE_ENV=test jest -i --detectOpenHandles",
    "start": "NODE_ENV=development webpack-dev-server --config webpack/development.config.js",
    "start-test": "NODE_ENV=test webpack-dev-server --config webpack/development.config.js",
    "start-prod": "NODE_ENV=production webpack-dev-server --config webpack/production.config.js",
    "dist": "shx mkdir -p ./build && NODE_ENV=production webpack -p --config webpack/production.config.js --progress",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "genRandomAccount": "yarn build && node ./build/scripts/genRandomAccount.js",
    "docker-build": "yarn dist && docker build --pull -t wallet-demo:${BUILD_VERSION:-manual} .",
    "docker-run": "docker run --read-only -p 3000:3000 wallet-demo:manual",
    "surge": "surge"
  },
  "dependencies": {
    "@iov/bns": "^0.13.0",
    "@iov/core": "^0.13.0",
    "@iov/crypto": "^0.13.0",
    "@iov/ethereum": "^0.13.0",
    "@iov/faucets": "^0.13.0",
    "@iov/keycontrol": "^0.13.0",
    "@iov/lisk": "^0.13.0",
    "@iov/stream": "^0.13.0",
    "@material-ui/core": "^3.5.1",
    "@material-ui/icons": "^3.0.1",
    "@sentry/browser": "4.5.2",
    "classnames": "^2.2.6",
    "config": "^2.0.1",
    "connected-react-router": "^6.0.0",
    "encoding-down": "^6.0.1",
    "file-saver": "^2.0.1",
    "final-form": "^4.11.0",
    "history": "^4.7.2",
    "level-js": "^4.0.0",
    "levelup": "^4.0.0",
    "lodash": "^4.17.11",
    "memdown": "^4.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.0",
    "postcss-preset-env": "^6.4.0",
    "postcss-safe-parser": "^4.0.1",
