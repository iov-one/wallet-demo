    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^3.1.0",
    "react-modal": "^3.6.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "styled-components": "^3.4.10",
