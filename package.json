{
  "name": "redux-demo",
  "version": "0.0.1",
  "description": "Demo of building typesafe redux",
  "main": "build/index.js",
  "author": "Ethan Frey <ethanfrey@users.noreply.github.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "tslint -t verbose --project .",
    "format": "prettier --write --loglevel warn \"./src/**/*.{ts,tsx}\"",
    "prebuild": "yarn format && yarn lint",
    "build": "shx rm -rf ./build && tsc --module commonjs",
    "test": "yarn build && yarn test-node",
    "test-node": "mocha build/*/*.spec.js build/*/*/*.spec.js",
    "start": "webpack-dev-server --config webpack/development.config.js",
    "start-prod": "webpack-dev-server --config webpack/production.config.js",
    "dist": "webpack -p --config webpack/production.config.js --progress",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "genRandomAccount": "yarn build && node ./build/scripts/genRandomAccount.js",
    "docker-build": "yarn dist && docker rmi -f wallet-demo:dev && docker build -t wallet-demo:dev .",
    "docker-run": "docker run -p 3000:80 wallet-demo:dev"
  },
  "dependencies": {
    "@iov/bns": "^0.7.0-alpha.1",
